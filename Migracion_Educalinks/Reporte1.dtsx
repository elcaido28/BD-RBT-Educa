                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">--select top 0 * from rpt_graduados_posgrado_SNIESE

select
(select id from education_levels where id=(select principal_id from education_levels where id=SR.education_level_id)) as 'niveles_educativos_id_facultad'
,(select id from education_levels where id=SR.education_level_id) as 'niveles_educativos_id_carrera'
,(select top 1 s.id from students S  inner join users U on U.id=S.user_id inner join persons P on P.id=U.person_id where S.id=SR.student_id) as 'alumno_id'
,(select V.DESCRIPTION FROM VW_CATALOGO_TYPE_IDENTIFICATION V where V.id=(select top 1 P.type_identification_id from students S  inner join users U on U.id=S.user_id inner join persons P on P.id=U.person_id where S.id=SR.student_id)) as 'tipo_identificacion'
,(select top 1 P.pers_date_birth from students S  inner join users U on U.id=S.user_id inner join persons P on P.id=U.person_id where S.id=SR.student_id) as 'fecha_nacimiento'
,(select V.DESCRIPTION FROM VW_CATALOGO_NATIONALITY V where V.id=(select top 1 P.nationality_id from students S  inner join users U on U.id=S.user_id inner join persons P on P.id=U.person_id where S.id=SR.student_id)) as 'pais_nacionalidad'
,CAST('ECUADOR' as nvarchar(10))  as 'pais_residencia'
,(select cat_name FROM catalogs where id=(select parent_id FROM catalogs where id=(select top 1 P.current_city_id from students S  inner join users U on U.id=S.user_id inner join persons P on P.id=U.person_id where S.id=SR.student_id))) as 'provincia_residencia'
,(select V.DESCRIPTION FROM VW_CATALOGO_CITY V where V.id=(select top 1 P.current_city_id from students S  inner join users U on U.id=S.user_id inner join persons P on P.id=U.person_id where S.id=SR.student_id)) as 'canton_residencia'
,(select pers_edu_certificate from person_educations PE where PE.id=SR.person_education_id ) as 'titulo'
,(select pers_edu_graduation_date from person_educations PE where PE.id=SR.person_education_id ) as 'fecha_graduacion'
from student_records SR
where SR.person_education_id is not null</property>
