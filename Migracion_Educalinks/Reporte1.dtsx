              <properties>
                <property
                  dataType="System.Int32"
                  description="Número de segundos antes de que se agote el tiempo de espera de un comando. Un valor de 0 indica un tiempo de espera infinito."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Especifica el nombre del objeto de base de datos usado para abrir un conjunto de filas."
                  name="OpenRowset">[dbo].[rpt_cartera_vencida_postgrado]</property>
                <property
                  dataType="System.String"
                  description="Especifica la variable que contiene el nombre del objeto de base de datos usado para abrir un conjunto de filas."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
	p.per_current_year  as 'periodo_anio',
	p.per_name  as 'cohorte',
	CONVERT(date ,p.start_process)as 'fecha_inicio',
	--e.principal_id as unidad_academica,	
	(select id from education_levels where id=e.principal_id) as 'niveles_educativos_id_facultad',
	(select id from education_levels where id=sr.education_level_id) as 'niveles_educativos_id_carrera',
	(select off_name from offers where id=e.offer_id) as 'oferta_academica',
	(select top 1 s.id from students S  inner join users U on U.id=S.user_id inner join persons P on P.id=U.person_id where S.id=d.student_id) as 'alumno_id',
	(select top 1 pers_email from students S  inner join users U on U.id=S.user_id inner join persons P on P.id=U.person_id where S.id=d.student_id) as 'email1',
	(select top 1 email from students S  inner join users U on U.id=S.user_id inner join persons P on P.id=U.person_id where S.id=d.student_id) as 'email2',
	(select fees_name_identifier from fees where fees.id = f.id ) as 'cuota',
	(select sum (fees.fees_net_amount) from fees where fees.id = f.id ) as 'VALOR',
	f.fees_expiration_date as 'fecha_vencimiento',
	datename(MM,f.fees_expiration_date)+' '+convert(nvarchar,year(fees_expiration_date)) as 'año_mes_vencimiento'
from debts d
inner join fees f on f.id_debts = d.id
inner join periods p on d.period_id = p.id
inner join student_recods_period srp on srp.periodo_id = p.id
inner join student_records sr on sr.student_id = d.student_id and sr.id = srp.student_record_id
inner join education_levels e on e.id = sr.education_level_id
where d.de_balance_value &gt; 0  and f.fees_expiration_date &lt; GETDATE() and e.offer_id=3</property>
