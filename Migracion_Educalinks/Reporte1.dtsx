                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">--select top 0 * from rpt_postgrado_retirados


select alumno_id, programa,programa as cohorte,   fecha_ini,observacion,CAST(estado as nvarchar(20)) estado
from(
select 
S.id as 'alumno_id'

,CASE 
WHEN (select COUNT(*) from course_student cs (NOLOCK) WHERE cs.student_record_id = sr.id) = 0 
then (select top 1 p.per_name 
from debts d (NOLOCK)
inner join periods p (NOLOCK) ON d.period_id = p.id
where d.student_id = s.id
order by de_date desc)
else (P.per_name) end as 'programa'

,CASE 
WHEN (select COUNT(*) from course_student cs (NOLOCK) WHERE cs.student_record_id = sr.id) = 0 
then (select top 1 d.de_date 
from debts d (NOLOCK)
inner join periods p (NOLOCK) ON d.period_id = p.id
where d.student_id = s.id
order by de_date desc)
else (select top 1 CONVERT(DATE,start_date) 
from course_student cs (NOLOCK)
inner join courses c (NOLOCK) ON c.id = cs.course_id 
where cs.student_record_id = SR.id 
order by start_date) end as 'fecha_ini'

,CASE 
WHEN (select COUNT(*) from course_student cs (NOLOCK) where SR.id = cs.student_record_id and cs.period_id = p.id and CS.approval_reason_id in (select id from VW_CATALOGO_REASON_COURSE (NOLOCK) WHERE Keyword in( 'motivo-aprobacion-retirado', 'motivo-aprobacion-retirado-fuerza-mayor') )) &gt; 0 then 'RETIRADO'
WHEN (s.status_id = 2) or (select COUNT(*) from course_student cs (NOLOCK) WHERE SR.id = cs.student_record_id and cs.period_id = p.id and CS.approval_reason_id in (select id from VW_CATALOGO_REASON_COURSE (NOLOCK) WHERE Keyword in( 'motivo-aprobacion-reprobado-por-calificacion', 'motivo-aprobacion-reprobado-falta', 'motivo-aprobacion-reprobado-falta-calificacion') )) &gt;0 then 'INACTIVO'
WHEN (select COUNT(*) from course_student cs (NOLOCK) where cs.student_record_id = SR.id) = 0 then 'DESISTIDO'
end   as estado
,S.stud_observation as 'observacion'
from student_records SR (NOLOCK) 
left join student_recods_period SRP (NOLOCK) ON SRP.student_record_id = SR.id
left join periods P (NOLOCK) ON P.id = SRP.periodo_id 
inner join students S (NOLOCK) ON S.id = SR.student_id 
inner join users u (NOLOCK) ON u.id = s.user_id
inner join persons pe (NOLOCK) ON u.person_id = pe.id
inner join education_levels EL (NOLOCK) ON EL.id = SR.education_level_id
where EL.offer_id=3 
group by 
S.id
,pe.pers_full_name
,P.per_name
,P.per_current_year
,sr.id
,S.stud_observation
,s.status_id
,p.id
)A
where estado is not null and programa is not null
order by estado, alumno_id</property>
